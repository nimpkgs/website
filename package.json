{
  "scripts": {
    "dev": "concurrently npm:server npm:uno:dev npm:watch npm:packageserver",
    "server": "http-server ./site --cors --port 8188",
    "packageserver": "http-server ../nimpkgs/site --cors --port 8189 --gzip",
    "watch": "nim watch",
    "build": "pnpm run uno:prd && pnpm run minify",
    "minify": "esbuild --minify --outdir=site --allow-overwrite site/app.js site/uno.css",
    "uno:dev": "unocss \"./site/**/*.html\" \"./src/**/*.nim\"  --out-file site/uno.css -w",
    "uno:prd": "unocss \"./site/**/*.html\" \"./src/**/*.nim\"  --out-file site/uno.css"
  },
  "author": "Daylin Morgan",
  "license": "MIT",
  "devDependencies": {
    "@catppuccin/palette": "^1.7.1",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/simple-icons": "^1.2.66",
    "@types/promise-fs": "^2.1.5",
    "@unocss/cli": "^66.5.12",
    "@unocss/preset-icons": "^66.5.12",
    "@unocss/reset": "^66.5.12",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.2",
    "http-server": "^14.1.1",
    "unocss": "^66.5.12"
  }
}
